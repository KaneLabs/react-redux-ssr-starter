{
  "name": "react-redux-ssr-starter",
  "version": "0.0.1",
  "description": "Server Side Rendering in React & Redux, w/ Async, React-Router, ES7+, Webpack, Babel",
  "main": "src/server.jsx",
  "scripts": {
    "start": "node build/build.js",
    "dev:server": "nodemon --watch build --exec \"node build/build.js\"",
    "dev:build:client": "webpack --config webpack.client.js --watch",
    "dev:build:server": "webpack --config webpack.server.js --watch",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.client.js",
    "build:server": "webpack --config webpack.server.js",
    "postinstall": "npm run build",
    "test": "mocha --recursive --require babel-core/register"
  },
  "author": "Kane Labs",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "express-http-proxy": "^1.1.0",
    "heroku-ssl-redirect": "0.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.5",
    "style-loader": "^0.20.2"
  }
}
